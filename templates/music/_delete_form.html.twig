<form method="post" action="{{ path('app_music_delete', {'id': music.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
   <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ music.id) }}"  >
   <div class="form-group">  <button   class="btn btn-primary">Delete</button></div>
</form>
